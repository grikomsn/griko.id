    "@types/nprogress": "^0.2.0",
    "@types/puppeteer": "^5.4.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/rss": "^0.0.28",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "copy": "^0.3.2",
    "env-cmd": "^10.1.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
