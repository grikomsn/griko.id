    "date-fns": "^2.16.1",
    "favicons": "^6.2.0",
    "framer-motion": "^3.1.1",
    "graphql": "^15.4.0",
    "graphql-request": "^3.4.0",
    "next": "^10.0.4",
    "next-seo": "^4.17.0",
    "nprogress": "^0.2.0",
    "prism-react-renderer": "^1.1.1",
    "puppeteer": "^5.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
